{
  "name": "launch-pack-scaffold",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:so-ta/launch-pack-scaffold.git",
  "scripts": {
    "start": "run-s start:local",
    "start:local": "npm-run-all -s build:local -p \"launch -- local\" browsersync watch",
    "start:develop": "run-s build:develop \"launch -- devlop\"",
    "start:staging": "run-s build:production \"launch -- staging\"",
    "start:production": "run-s build:production \"launch -- production\"",
    "build": "run-s build:local",
    "build:local": "run-s build:develop",
    "build:develop": "run-s clean webpack generate-resourcesmap",
    "build:staging": "run-s build:production",
    "build:production": "cross-env NODE_ENV=production run-s clean webpack generate-resourcesmap",
    "clean": "rimraf public",
    "webpack": "webpack",
    "generate-resourcesmap": "gulp generateResourcesmap",
    "launch": "launch-pack",
    "watch": "gulp watch",
    "browsersync": "browser-sync start --proxy localhost:1337 --files public/**/* --files views/**/*"
  },
  "author": "so-ta",
  "license": "UNLICENSED",
  "private": false,
  "dependencies": {
    "jquery": "^3.4.1",
    "moment-timezone": "^0.5.28"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "child_process": "^1.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.2",
    "gulp-watch": "^5.0.1",
    "launch-pack-core": "^1.11.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sha256-file": "^1.0.0",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.5"
  }
}
